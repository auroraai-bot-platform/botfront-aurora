  "scripts": {
    "start": "meteor run",
    "start:docker-compose": "BF_PROJECT_ID=bf meteor run",
    "start:docker-compose.dev": "MODE=development ADMIN_USER=test@test.com ADMIN_PASSWORD=Aaaaaaaa00 BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf meteor run",
    "start:docker-compose.ci": "MODE=test BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf meteor run",
    "test-watch": "FILE_BUCKET=test METEOR_PORT=3100 TEST_BROWSER_DRIVER=chrome meteor test --driver-package meteortesting:mocha --port 3100",
    "start:gke": "ORCHESTRATOR=gke meteor run",
    "start:gke.dev": "MODE=development BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf ORCHESTRATOR=gke meteor run",
    "start:gke.ci": "MODE=test CI=true BF_PROJECT_ID=bf MONGO_URL=mongodb://localhost:27017/bf ORCHESTRATOR=gke meteor run",
    "test-once": "FILE_BUCKET=test METEOR_PORT=3100 TEST_BROWSER_DRIVER=chrome meteor test --once --driver-package meteortesting:mocha --port 3100",
    "test-rest": "FILE_BUCKET=test TEST_CLIENT=0 MOCHA_GREP=.+REST:.+ METEOR_PORT=3100 TEST_BROWSER_DRIVER=chrome meteor test --driver-package meteortesting:mocha --port 3100",
    "debug": "MODE=development BF_PROJECT_ID=3kquhKhqDChbRH7C4 MONGO_URL=mongodb://localhost:27017/bf meteor run",
    "test:e2e": "CYPRESS_RETRIES=0 npx cypress run",
    "ci": "CI=true start-server-and-test start http://localhost:3000 test:e2e",
    "licenses:summary": "license-checker --summary",
    "licenses:csv": "license-checker --csv --out botfront-licenses.csv",
    "release": "standard-version",
    "storybook": "start-storybook",
    "postinstall": "postinstall.sh"
